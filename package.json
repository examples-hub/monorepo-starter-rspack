{
  "name": "monorepo-starter-rspack",
  "version": "1.1.1111",
  "private": true,
  "description": "quickstart monorepo example for js/ts/react projects",
  "author": "jinyaoo <jinyaoo@qq.com>",
  "type": "module",
  "workspaces": [
    "packages/foo",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "npm run build:dev --workspace=packages && npm run build:dev --workspace=packages",
    "//build:twice": "echo 'build twice to avoid stale dependent packages dist'",
    "build:twice": "npm run build --workspace=packages && npm run build --workspace=packages",
    "build:prod": "npm run build:twice",
    "build:tsc": "npm run clean && tsc -b -v -w ./tsconfig.json",
    "clean": "rimraf ./dist && npm run clean --workspace=packages --workspace=apps",
    "demo": "cross-env REACT_APP_ENV=reacthot rspack serve -c ./webpacker/rspack.config.demo.mjs",
    "dev": "npm run build:tsc",
    "start": "npm run demo",
    "test": "cross-env REACT_APP_ENV=react jest --config ./jest.config.js",
    "test1": "pnpm run -r test",
    "type-check": "tsc --noEmit",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,mjs,mts,cjs,json}\" --write",
    "format:check": "npm run formatter -- --check",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "lint1": "eslint --ext js,ts,tsx .",
    "syncpack-fix": "syncpack-fix-mismatches",
    "syncpack-ls": "syncpack-list-mismatches",
    "help": "echo 'exec npm run build to build all projects'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@faker-js/faker": "^8.3.1",
    "cross-fetch": "^4.0.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.4",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tinycolor2": "^1.6.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rspack/cli": "0.4.5-canary-9d36121-20240103112254",
    "@rspack/core": "0.4.5-canary-9d36121-20240103112254",
    "@rspack/plugin-react-refresh": "0.4.5-canary-9d36121-20240103112254",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "chai": "^5.0.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.35.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mdx": "^3.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "global-jsdom": "^9.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^23.0.1",
    "less": "^4.2.0",
    "less-loader": "^11.1.4",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.2.1",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "source-map-loader": "^4.0.2",
    "style-loader": "^3.3.3",
    "syncpack": "^12.3.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.3.0"
  }
}
